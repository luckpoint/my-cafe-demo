        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "lib": [
            "ESNext"
        ],
        "types": [
            "@cloudflare/workers-types",
            "hono"
        ],
